le\nfun CheckoutScreen(\n    onBack: () -\u003e Unit,\n    totalPrice : Double,\n    subtotal: Double,\n    estimatedFee: Double,\n    itemsCount: Int,\n    items: List\u003cOrderItem\u003e,\n    onConfirmOrder: () -\u003e Unit,\n    chekcoutViewModel: CheckoutViewModel \u003d hiltViewModel(),\n) {\n    var selectedPaymentMethod by remember { mutableStateOf(PaymentMethod.CASH) }\n    val location by chekcoutViewModel.selectedLocation.collectAsState()\n    val selectedAddress by chekcoutViewModel.selectedAddress.collectAsState()\n\n    Log.d(\&quot;CheckoutScreen\&quot;, \&quot;CheckoutScreen: $location\&quot;)\n    Scaffold(\n        topBar \u003d {\n            CustomTopAppBar(\n                title \u003d \&quot;Checkout\&quot;,\n                onBackClick \u003d onBack,\n            )\n        },\n        bottomBar \u003d {\n            ConfirmOrderBottomBar(onConfirmOrder \u003d onConfirmOrder)\n        }\n    ) { paddingValues -\u003e\n        Column(\n            modifier \u003d Modifier\n                .fillMaxSize()\n                .padding(paddingValues)\n                .verticalScroll(rememberScrollState())\n                .background(Color.White)\n        ) {\n            val location2 \u003d LatLng(location?.latitude ?: 0.0, location?.longitude ?: 0.0)\n            MapAndAddressSection(location2,\n                address \u003d selectedAddress.toString()\n            )\n            PayWithSection(\n                selectedMethod \u003d selectedPaymentMethod,\n                onMethodSelected \u003d { selectedPaymentMethod \u003d it }\n            )\n            Spacer(Modifier.height(16.dp))\n\n            PaymentSummarySection(\n                subtotal \u003d subtotal,\n                estimatedFee \u003d estimatedFee,\n                itemsCount \u003d itemsCount,\n                totalPrice \u003d totalPrice\n            )\n            Spacer(Modifier.height(24.dp))\n        }\n    }\n}\n\n@Composable\nprivate fun MapAndAddressSection(location: